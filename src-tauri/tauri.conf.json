{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "rbw-vpn-client",
  "version": "0.1.0",
  "identifier": "com.radip.rbw-vpn-client",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "RBW-TECH OVPN",
        "width": 420,
        "height": 680,
        "resizable": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["rbw-vpn-client.exe.manifest", "resources/*"],
    "windows": {
      "wix": {
        "language": "en-US"
      },
      "nsis": {
        "installerIcon": "icons/icon.ico"
      }
    }
  }
}
